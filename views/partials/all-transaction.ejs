<% for(let i=0; i<ListTransaction.length; i++){%>
<div class="row">
  <div class="col s12 m12">
     <div class="card horizontal">
       <div class="card-image left-card">
         <div class="center-align">
           <h4><%- ListTransaction[i].tanggal_transaksi.getDay() %></h4>
           <p><%- ListTransaction[i].tanggal_transaksi.getMonth() %>  <%- ListTransaction[i].tanggal_transaksi.getFullYear() %></p>
         </div>
       </div>
       <div class="card-stacked">
         <div class="card-content">
           <span class="card-title activator grey-text text-darken-4">Rp <%- ListTransaction[i].amount %></span>
           <p><%- ListTransaction[i].keterangan %></p>
           <p><%- ListTransaction[i].Category.nama_category %></p>
           <a href="/user/dashboard/editData/<%- ListTransaction[i].id %>">Edit</a>
         </div>
     </div>
    </div>
 </div>
<% } %>
